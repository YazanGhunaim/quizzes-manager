<?xml version="1.0" encoding="UTF-8"?>
<quiz title="main quiz">
    <section title="world war knowledge">
        <question type="text">
            <text>Germany won WW1</text>
            <answer type="T/F">
                <text>false</text>
            </answer>
        </question>
        <question type="single">
            <text>When was WW1?</text>
            <options>
                <text>1914-1918</text>
                <text>1915-1919</text>
                <text>1916-1920</text>
                <text>1917-1921</text>
            </options>
            <answer type="interval">
                <text>1914-1918</text>
            </answer>
        </question>
        <question type="multi">
            <text>Who were the allies in WW1</text>
            <options>
                <text>France UK Italy Japan USA</text>
                <text>Germany Russia Jordan</text>
                <text>Italy France UK Germany</text>
                <text>There were no allies</text>
            </options>
            <answer type="text">
                <text>France UK Italy Japan USA</text>
            </answer>
        </question>
        <question type="text">
            <text>who was the dictator of Germany from 1933 until his suicide in 1945.</text>
            <answer type="text">
                <text>adolf hitler</text>
            </answer>
        </question>
    </section>
